    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^35.0.0",
    "electron-rebuild": "^3.2.9",
    "html-webpack-plugin": "^5.5.3",
    "node-loader": "^2.0.0",
